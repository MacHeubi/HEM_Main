CREATE TABLE  "CS_SNIPPETS" 
   ("ID" NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  NOSCALE  NOT NULL ENABLE, 
	"NAME" VARCHAR2(80) COLLATE "USING_NLS_COMP", 
	"DESCRIPTION" VARCHAR2(400) COLLATE "USING_NLS_COMP", 
	"GROUP_ID" NUMBER, 
	"CODE" VARCHAR2(4000) COLLATE "USING_NLS_COMP", 
	"ADVANCED" NUMBER, 
	"PRIVATE" NUMBER,
	"INSERT_DATE" DATE, 
	"OWNER" VARCHAR2(80) COLLATE "USING_NLS_COMP", 
	 PRIMARY KEY ("ID")
  USING INDEX  ENABLE
   )  DEFAULT COLLATION "USING_NLS_COMP"
/